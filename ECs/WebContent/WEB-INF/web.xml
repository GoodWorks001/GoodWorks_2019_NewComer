<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ECs</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<!-- login -->
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>ec.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<!-- search -->
	<servlet>
		<servlet-name>searchresult</servlet-name>
		<servlet-class>ec.SearchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>searchresult</servlet-name>
		<url-pattern>/searchresult</url-pattern>
	</servlet-mapping>
	<!--sinasyousai -->
	<servlet>
		<servlet-name>sinasyousai</servlet-name>
		<servlet-class>ec.SinaSyousaiServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>sinasyousai</servlet-name>
		<url-pattern>/sinasyousai</url-pattern>
	</servlet-mapping>

	<!--cart -->
	<servlet>
		<servlet-name>cart</servlet-name>
		<servlet-class>ec.CartServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>cart</servlet-name>
		<url-pattern>/cart</url-pattern>
	</servlet-mapping>

	<!--kou_check -->
	<servlet>
		<servlet-name>kou_check</servlet-name>
		<servlet-class>ec.kou_checkServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>kou_check</servlet-name>
		<url-pattern>/kou_check</url-pattern>
	</servlet-mapping>

	<!--kanryou -->
	<servlet>
		<servlet-name>kanryou</servlet-name>
		<servlet-class>ec.FinishServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>kanryou</servlet-name>
		<url-pattern>/kanryou</url-pattern>
	</servlet-mapping>

	<!--logout -->
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>ec.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<!--start -->
	<servlet>
		<servlet-name>start</servlet-name>
		<servlet-class>ec.StartServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>start</servlet-name>
		<url-pattern>/start</url-pattern>
	</servlet-mapping>
</web-app>